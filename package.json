{
  "name": "@peter.naydenov/dom-selector",
  "description": "DOM selections and DOM references organizer",
  "version": "3.0.0",
  "license": "MIT",
  "author": "Peter Naydenov",
  "main": "./src/main.js",
  "type": "module",
  "types": "./types/main.d.ts",
  "exports": {
    ".": {
      "import": "./src/main.js",
      "require": "./dist/dom-selector.cjs",
      "default": "./src/main.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./src/*": "./src/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "rollup -c",
    "postbuild": "tsc -p tsconfig.json",
    "preview": "vite preview",
    "test": "cypress run --component --browser chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeterNaydenov/dom-selector.git"
  },
  "devDependencies": {
    "@peter.naydenov/visual-controller-for-vue3": "^2.1.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.21",
    "@vitejs/plugin-vue": "^5.2.4",
    "chai": "^5.2.0",
    "cypress": "^14.4.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vue": "^3.5.16"
  },
  "keywords": [
    "dom",
    "select",
    "html",
    "scan"
  ]
}
